{
  "name": "guleboda.se",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "classnames": "^2.2.5",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-iframe": "^1.0.7",
    "react-intl": "^2.3.0",
    "react-intl-translations-manager": "^5.0.0",
    "react-lorem-component": "^0.12.1",
    "react-parallax": "^1.4.1",
    "react-photoswipe": "^1.3.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint-config-airbnb": "^15.0.1",
    "node-sass-chokidar": "0.0.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css watch-trans start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "trans:extract": "babel \"./src/**/*.jsx\"",
    "trans:manage": "node scripts/translationRunner.js",
    "trans": "npm run trans:extract && npm run trans:manage",
    "watch-trans": "nodemon -e jsx --exec \"npm run trans\""
  }
}
